    "rc-swipeout": "~1.1.4",
    "rc-table": "~4.4.2",
    "rc-tabs": "~5.9.2",
    "rc-tooltip": "~3.4.2",
    "react-hammerjs": "^0.5.0",
    "react-native-camera-roll-picker": "^1.1.6",
    "react-native-drawer-layout": "~1.0.0",
    "react-native-menu": "~0.20.1",
    "react-slick": "~0.12.2",
    "rmc-cascader": "~2.2.3",
    "rmc-date-picker": "~4.0.1",
    "rmc-list-view": "~0.6.1",
