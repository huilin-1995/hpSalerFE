{
  "name": "css-vendor",
  "description": "CSS vendor prefix detection and property feature testing.",
  "version": "0.3.5",
  "author": {
    "name": "Oleg Slobodskoi",
    "email": "oleg008@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cssinjs/css-vendor.git"
  },
  "keywords": [
    "css",
    "vendor",
    "feature",
    "test",
    "prefix"
  ],
  "engines": {},
  "scripts": {
    "all": "npm run lint && npm run build",
    "test": "opener ./test/index.html",
    "build": "npm run clean && npm run build:lib && npm run build:max && npm run build:min",
    "clean": "rimraf ./lib/*",
    "build:lib": "babel src --out-dir lib",
    "build:max": "NODE_ENV=development webpack src/index.js dist/css-vendor.js",
    "build:min": "NODE_ENV=production webpack src/index.js dist/css-vendor.min.js",
    "lint": "eslint ./src",
    "prepublish": "npm run all && git push --tags"
  },
  "license": "MIT",
  "main": "./lib/index",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-jss": "^1.1.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "jss": "^5.4.0",
    "opener": "^1.4.1",
    "qunitjs": "1.21.0",
    "rimraf": "^2.5.1",
    "webpack": "^2.0.7-beta"
  },
  "dependencies": {
    "is-browser": "^2.0.1"
  },
  "_from": "css-vendor@0.3.5",
  "_resolved": "http://registry.npm.taobao.org/css-vendor/download/css-vendor-0.3.5.tgz"
}